<Window x:Class="OrderManagement.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OrderManagement.Client"
        xmlns:fa="clr-namespace:Meziantou.WpfFontAwesome;assembly=Meziantou.WpfFontAwesome"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50"></ColumnDefinition>
            <ColumnDefinition ></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Background="#2b2b2b">
            <fa:FontAwesomeIcon Margin="0, 8" VerticalAlignment="Top" SolidIcon="Box" FontSize="20" Foreground="White"/>
            <fa:FontAwesomeIcon Margin="0, 8" VerticalAlignment="Top" SolidIcon="Boxes" FontSize="20" Foreground="White"/>
            <fa:FontAwesomeIcon Margin="0, 8" VerticalAlignment="Top" SolidIcon="FolderOpen" FontSize="20" Foreground="White"/>
            <fa:FontAwesomeIcon Margin="0, 8" VerticalAlignment="Top" SolidIcon="ListUl" FontSize="20" Foreground="White"/>
            <fa:FontAwesomeIcon Margin="0, 8" VerticalAlignment="Top" SolidIcon="User" FontSize="20" Foreground="White"/>
            <fa:FontAwesomeIcon Margin="0, 8" VerticalAlignment="Top" SolidIcon="AddressCard" FontSize="20" Foreground="White"/>
        </StackPanel>
        <Grid Grid.Column="1" Background="#444444">

        </Grid>
    </Grid>
</Window>
